    <Entry>
      <PointerOffset>2163,2146</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2180</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;さん！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2256</PointerOffset>
      <JapaneseText>&lt;voice:19000010&gt;おーい。大丈夫？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
