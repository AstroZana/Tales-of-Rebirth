    <Entry>
      <PointerOffset>4403</PointerOffset>
      <JapaneseText>&lt;voice:0E000013&gt;おい&lt;Mao&gt;、起きろ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4424</PointerOffset>
      <JapaneseText>&lt;voice:0E000014&gt;……うーん……
ううっ……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4445</PointerOffset>
      <JapaneseText>&lt;voice:0E000015&gt;おい、&lt;Mao&gt;の様子が変だぞ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4466</PointerOffset>
      <JapaneseText>&lt;voice:0E000016&gt;……七つの力を……
ボクは目……ボクは……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>447</PointerOffset>
      <JapaneseText>金がないのに、
泊まる気だったのかい？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4525</PointerOffset>
      <JapaneseText>&lt;voice:0E000017&gt;七つの力……
何の事だ……？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>455</PointerOffset>
      <JapaneseText>……ふん……
ひやかしかい……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4562</PointerOffset>
      <JapaneseText>&lt;voice:0E000018&gt;……いつもこの調子だ……
もしかしたら、失ったはずの記憶を
夢に見ているのではないかと
思っているんだが……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4597</PointerOffset>
      <JapaneseText>&lt;voice:0E000019&gt;……起きろ、&lt;Mao&gt;。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>466</PointerOffset>
      <JapaneseText>&lt;font:80000002&gt;一泊{17}{34}{0C}{80}ガルドだけど、
泊まるかい？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4818</PointerOffset>
      <JapaneseText>&lt;voice:0E00001A&gt;&lt;Veigue&gt;、危ない!!</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>482</PointerOffset>
      <JapaneseText>いってらっしゃい。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>490</PointerOffset>
      <JapaneseText>お金が足りないよ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4950</PointerOffset>
      <JapaneseText>&lt;voice:0E00001B&gt;おい、なんのつもりだ!!</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4972</PointerOffset>
      <JapaneseText>&lt;voice:0E00001C&gt;…………</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>498,714</PointerOffset>
      <JapaneseText>またおいで。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5113</PointerOffset>
      <JapaneseText>&lt;voice:0E00001D&gt;&lt;Mao&gt;、敵だ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5181</PointerOffset>
      <JapaneseText>&lt;voice:0E00001E&gt;えっ!?</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5292</PointerOffset>
      <JapaneseText>&lt;voice:0E00001F&gt;でも、フォルスは感じないよ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5349</PointerOffset>
      <JapaneseText>&lt;voice:0E000020&gt;だが、あの動きは
普通じゃなかった。
&lt;Blue&gt;王の盾&lt;White&gt;の能力者か？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5391</PointerOffset>
      <JapaneseText>&lt;voice:0E000021&gt;いや……もしや……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5418</PointerOffset>
      <JapaneseText>&lt;voice:0E000022&gt;……？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5447</PointerOffset>
      <JapaneseText>&lt;voice:0E000023&gt;とにかく、追いかけようよ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5543</PointerOffset>
      <JapaneseText>&lt;voice:0E000043&gt;ん……時計？
そういえば、さっき……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5639</PointerOffset>
      <JapaneseText>&lt;voice:0E000044&gt;どうだ、&lt;Mao&gt;？
&lt;Eugene&gt;？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5692</PointerOffset>
      <JapaneseText>&lt;voice:0E000045&gt;アタリだよ、&lt;Veigue&gt;!!</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5728</PointerOffset>
      <JapaneseText>&lt;voice:0E000046&gt;やったな。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>582,798,311,365,528,636,257,204</PointerOffset>
      <JapaneseText>ごゆっくりお休みください。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5823</PointerOffset>
      <JapaneseText>&lt;voice:0E000047&gt;やれやれだ……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5912</PointerOffset>
      <JapaneseText>&lt;voice:0E000048&gt;安心するのは
まだ早いみたいだね……
近いよ。外だ!!</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>682</PointerOffset>
      <JapaneseText>&lt;font:80000002&gt;一泊{17}{34}{0C}{80}ガルドだけど、
泊まる？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>698</PointerOffset>
      <JapaneseText>よく眠れたかい？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>706</PointerOffset>
      <JapaneseText>お金が足りないみたいね。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>736</PointerOffset>
      <JapaneseText>&lt;font:80000002&gt;一泊{17}{34}{0C}{80}ガルドになりますが、
よろしいでしょうか？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
