    <Entry>
      <PointerOffset>1203</PointerOffset>
      <JapaneseText>&lt;Green&gt;&lt;scale:0140&gt;レバーを倒しました。</JapaneseText>
      <EnglishText>&lt;Green&gt;&lt;scale:0140&gt;You pulled the lever.</EnglishText>
      <Notes/>
      <Id>4</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1429</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
