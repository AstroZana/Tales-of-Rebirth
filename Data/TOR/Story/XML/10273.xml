    <Entry>
      <PointerOffset>912</PointerOffset>
      <JapaneseText>クレア……来てくれたのね。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>934</PointerOffset>
      <JapaneseText>みんな仲が悪くなって
ケンカばかりするように
なってしまったの……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>956</PointerOffset>
      <JapaneseText>前みたいに仲良くなって
ほしいんだけど、</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9680,9683</PointerOffset>
      <JapaneseText>ミナール</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>978</PointerOffset>
      <JapaneseText>私もガジュマが怖くて、
私……どうすればいいの？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
