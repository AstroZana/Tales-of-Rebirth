<SceneText>
  <Speakers>
    <Section>Speaker</Section>
  </Speakers>
  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>2882</PointerOffset>
      <JapaneseText>&lt;Hilda&gt;さんっておいくつなんですか？</JapaneseText>
      <EnglishText>How old are you, &lt;Hilda&gt;?</EnglishText>
      <Notes/>
      <Id>1306</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2901</PointerOffset>
      <JapaneseText>私……？　私は２１だけど……</JapaneseText>
      <EnglishText>Me...? I'm twenty-one...</EnglishText>
      <Notes/>
      <Id>1307</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2920</PointerOffset>
      <JapaneseText>わぁー、大人ですねー。</JapaneseText>
      <EnglishText>Oh, so you're all grown up!</EnglishText>
      <Notes/>
      <Id>1308</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2939</PointerOffset>
      <JapaneseText>……あんたは……？</JapaneseText>
      <EnglishText>How about you?</EnglishText>
      <Notes/>
      <Id>1309</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2958</PointerOffset>
      <JapaneseText>わたしは１５歳です。</JapaneseText>
      <EnglishText>I'm fifteen years old.</EnglishText>
      <Notes/>
      <Id>1310</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2978</PointerOffset>
      <JapaneseText>……そう、まだまだ、お子様ね……</JapaneseText>
      <EnglishText>I see, so you're still a kid.</EnglishText>
      <Notes/>
      <Id>1311</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2999</PointerOffset>
      <JapaneseText>…………</JapaneseText>
      <EnglishText>...</EnglishText>
      <Notes/>
      <Id>1312</Id>
      <Status>Editing</Status>
    </Entry>
  </Strings>
</SceneText>
