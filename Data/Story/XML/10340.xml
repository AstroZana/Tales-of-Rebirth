    <Entry>
      <PointerOffset>5076</PointerOffset>
      <JapaneseText>&lt;Annie&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5079</PointerOffset>
