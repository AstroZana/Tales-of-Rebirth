    <Entry>
      <PointerOffset>533</PointerOffset>
      <JapaneseText>ここには
いないみたいね……
</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>556</PointerOffset>
      <JapaneseText>{19}{D0}{FF}{1F}{C8}{22}{E0}{0C}{80}</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>559</PointerOffset>
      <JapaneseText>あの娘はいないか……
</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
