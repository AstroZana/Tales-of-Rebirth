    <Entry>
      <PointerOffset>2853</PointerOffset>
      <JapaneseText>&lt;name:00000005&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2856</PointerOffset>
      <JapaneseText>ありふれてるもの？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
