      <JapaneseText>民家</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>646</PointerOffset>
      <JapaneseText>民家</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1509</PointerOffset>
      <JapaneseText></JapaneseText>
