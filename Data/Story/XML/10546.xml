      <PointerOffset>2948</PointerOffset>
      <JapaneseText>女の人</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2956</PointerOffset>
      <JapaneseText>ザピィ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2964</PointerOffset>
      <JapaneseText>女の人</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2972</PointerOffset>
      <JapaneseText>クウ・ホウ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3021</PointerOffset>
      <JapaneseText>&lt;voice:2C0000C1&gt;キキッ!!</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3129</PointerOffset>
      <JapaneseText>&lt;voice:2C0000C2&gt;何……??</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
