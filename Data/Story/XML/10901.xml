    <Entry>
      <PointerOffset>279</PointerOffset>
      <JapaneseText>聖火台</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1329</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1332</PointerOffset>
      <JapaneseText>&lt;voice:29000000&gt;このあたりだな。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1384</PointerOffset>
      <JapaneseText>&lt;name:00000002&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1387</PointerOffset>
      <JapaneseText>&lt;voice:29000001&gt;何だろ、この台……？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1659</PointerOffset>
      <JapaneseText>&lt;name:00000003&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1662</PointerOffset>
      <JapaneseText>&lt;voice:29000002&gt;聖火台のようだな。
古代の人々は、ここで何かの
儀式を行っていたのかもしれん。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1694</PointerOffset>
      <JapaneseText>&lt;name:00000002&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1697</PointerOffset>
      <JapaneseText>&lt;voice:29000003&gt;だったら、同じ儀式をやれば、
何かがわかるかもしれないネ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1730</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1733</PointerOffset>
      <JapaneseText>&lt;voice:29000004&gt;ああ……だが、どうすれば……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1760</PointerOffset>
      <JapaneseText>&lt;name:00000004&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1763</PointerOffset>
      <JapaneseText>&lt;voice:29000005&gt;&lt;color:00000005&gt;聖壇&lt;color:00000007&gt;に書かれてた、
「強き心に強き翼を持て」って言葉に
何かありそうだと思いませんか？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1802</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1805</PointerOffset>
      <JapaneseText>&lt;voice:29000006&gt;強き心、強き翼、か……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2100</PointerOffset>
      <JapaneseText>レイ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2108</PointerOffset>
      <JapaneseText>トッポ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2169</PointerOffset>
      <JapaneseText>&lt;name:00000002&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2172</PointerOffset>
      <JapaneseText>&lt;voice:38000045&gt;あっ、いたヨ！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2414</PointerOffset>
      <JapaneseText>&lt;name:00000004&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2417</PointerOffset>
      <JapaneseText>&lt;voice:38000046&gt;あなたたち、
ピピスタから来たのよね？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2446</PointerOffset>
      <JapaneseText>&lt;voice:38000047&gt;そうだけど、なに？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2480</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2483</PointerOffset>
      <JapaneseText>&lt;voice:38000048&gt;おまえたちがいなくなって、
村は大騒ぎになっている。
すぐに戻ろう。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2520</PointerOffset>
      <JapaneseText>&lt;voice:38000049&gt;ヤダ！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2538</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2541</PointerOffset>
      <JapaneseText>&lt;voice:3800004A&gt;どうして？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2572</PointerOffset>
      <JapaneseText>&lt;voice:3800004B&gt;だって、おうちに帰ったら
レイと遊べなくなるもん。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2594</PointerOffset>
      <JapaneseText>&lt;voice:3800004C&gt;わたしもイヤよ！
みんな、怒ってばっかりで、
村にいても楽しくないもん。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2616</PointerOffset>
      <JapaneseText>&lt;voice:3800004D&gt;だから、ぼくたち、
&lt;color:00000006&gt;聖なる鳥&lt;color:00000007&gt;様にお願いしようと
思って、ここに来たんだよ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2638</PointerOffset>
      <JapaneseText>&lt;voice:3800004E&gt;みんなが仲良くなりますように。
レイと一緒に遊べますようにって……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2901</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2904</PointerOffset>
      <JapaneseText>&lt;voice:3800004F&gt;あなたたちの気持ちは
よくわかったわ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2963</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2966</PointerOffset>
      <JapaneseText>&lt;voice:38000050&gt;でもね、お父さんやお母さんを
心配させるのは良くないと思うの。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3009</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3012</PointerOffset>
      <JapaneseText>&lt;voice:38000051&gt;お祈りするのも悪くないけど、
あなたたちの気持ちを、
村のみんなに伝えたらどうかしら？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3034</PointerOffset>
      <JapaneseText>&lt;voice:38000052&gt;え、どうすればいいの？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3062</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3065</PointerOffset>
      <JapaneseText>&lt;voice:38000053&gt;あなたたちが仲良くしてる事、
どんな姿をしてても
仲良くできる事を、
教えてあげればいいのよ。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3102</PointerOffset>
      <JapaneseText>&lt;voice:38000054&gt;どうする……レイ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3133</PointerOffset>
      <JapaneseText>&lt;voice:38000055&gt;……お姉さんの言うとおりに
やってみようよ……トッポ……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3155</PointerOffset>
      <JapaneseText>&lt;voice:38000056&gt;うん、そうだね。
いつもいっしょに遊びたいもんね。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3174</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3177</PointerOffset>
      <JapaneseText>&lt;voice:38000057&gt;がんばって！
私たちも応援するから。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3196</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3199</PointerOffset>
      <JapaneseText>&lt;voice:38000058&gt;さあ、ピピスタへ帰りましょう。</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3239</PointerOffset>
      <JapaneseText>&lt;voice:38000059&gt;うん！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3256</PointerOffset>
      <JapaneseText>うん！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3274</PointerOffset>
      <JapaneseText>&lt;name:00000001&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3277</PointerOffset>
      <JapaneseText>&lt;voice:3800005C&gt;&lt;name:00000007&gt;……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3319</PointerOffset>
      <JapaneseText>&lt;name:00000008&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3322</PointerOffset>
      <JapaneseText>&lt;voice:3800005D&gt;うん……</JapaneseText>
      <EnglishText></EnglishText>
